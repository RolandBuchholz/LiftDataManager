<Page
    x:Class="LiftDataManager.Views.SignalisationPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:SpeziControls="using:LiftDataManager.Controls"
    xmlns:controls="using:CommunityToolkit.WinUI.UI.Controls"
    xmlns:converters="using:CommunityToolkit.WinUI.UI.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:LiftDataManager.Helpers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:media="using:CommunityToolkit.WinUI.UI.Media"
    xmlns:ui="using:CommunityToolkit.WinUI.UI"
    mc:Ignorable="d">

    <Page.Resources>
        <media:AttachedCardShadow
            x:Key="CommonShadow"
            CornerRadius="5"
            Offset="5" />
        <converters:BoolToObjectConverter
            x:Key="InvertBoolToObjectConverter"
            FalseValue="Visible"
            TrueValue="Collapsed" />
        <helpers:NewLineConveter x:Key="NewLineConveter" />
        <helpers:CustomInputStringConveter x:Key="CustomInputStringConveter" />
        <helpers:StringToBoolConverter x:Key="StringToBoolConverter" />
    </Page.Resources>
    <Grid x:Name="ContentArea">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <!--  Parameter  -->
        <ScrollViewer
            Grid.Row="0"
            Margin="0,0,0,0"
            Padding="0,0,15,0">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Expander
                    x:Name="KDKT"
                    Grid.Row="0"
                    Margin="0,0,0,10"
                    Header="Kabinendruckknopftableau"
                    IsExpanded="True"
                    Style="{StaticResource ExpanderContentStyle}">
                    <Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,*">
                        <Grid x:Name="Kabinentableau" Grid.Row="0">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="4"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="3"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Kabinentableau" />
                                <Grid
                                    x:Name="KDKT0"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,2*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        Header="Typ"
                                        LiftParameter="{x:Bind ViewModel.ParamterDictionary['var_KabTabKabinentableau'], Mode=TwoWay}" />
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        Header="Zusatzinformationen"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_FT_Info1'].Value, Mode=TwoWay}" />
                                </Grid>
                                <Grid
                                    x:Name="KDKT1"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,2*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        Header="Aufbau"
                                        LiftParameter="{x:Bind ViewModel.ParamterDictionary['var_KabTabAufbau'], Mode=TwoWay}" />
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        Header="Zusatzinformationen"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_FT_Info2'].Value, Mode=TwoWay}" />
                                </Grid>
                                <Grid
                                    x:Name="KDKT2"
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,2*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        Header="Material"
                                        LiftParameter="{x:Bind ViewModel.ParamterDictionary['var_KabTabMaterial'], Mode=TwoWay}" />
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        Header="Ral-Farbe"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_FT_RAL'].Value, Mode=TwoWay}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Standanzeiger"
                            Grid.Row="1"
                            Margin="0,5,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="3"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="2"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Standanzeiger" />
                                <Grid
                                    x:Name="KDKT3"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,*,*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        Header="Punktmatrix Typ"
                                        LiftParameter="{x:Bind ViewModel.ParamterDictionary['var_KabTabPunktmatrixTyp'], Mode=TwoWay}" />
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="1"
                                        Header="Punktmatrix Farbe"
                                        LiftParameter="{x:Bind ViewModel.ParamterDictionary['var_KabTabFarbe'], Mode=TwoWay}" />
                                    <TextBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        Header="TFT-Bildschirm Typ "
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_KabTabTFTBildschirmTyp'].Value, Mode=TwoWay}" />
                                </Grid>
                                <Grid
                                    x:Name="KDKT4"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,*,*">
                                    <CheckBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Pfeile scrollend"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabPfeilescrollend'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Pfeile statisch"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabPfeilestat'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <TextBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        Header="LCDAnzeigeTyp"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_KabTabLCDAnzeigeTyp'].Value, Mode=TwoWay}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="GravurenTexte"
                            Grid.Row="2"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="3"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="2"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Gravuren Texte" />

                                <Grid
                                    x:Name="KDKT5"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,*,*">
                                    <CheckBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Aufzug im Brandfall nicht benutzen"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabAufzugimBrandfall'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Firmenlogo"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabFirmenlogo'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Hinweis Notrufbenutzung"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabHinweisNotrufbenutzung'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                </Grid>
                                <Grid
                                    x:Name="KDKT6"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,*,*">
                                    <CheckBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Piktogr EN81-73"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabPiktogrEN8173'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Aufzugdaten"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabAufzugdaten'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Schlüsselschalter"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabSchluesselschalter'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Gegensprechanlage"
                            Grid.Row="3"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Gegensprechanlage" />
                                <Grid
                                    x:Name="KDKT7"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*,*,*">
                                    <CheckBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Kabine"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabKabine'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Steuerung"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabSteuerung'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Schachtgrube"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabSchachtgr'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="3"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="mit Notruf kombiniert"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabmitNotrufkombiniert'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Sprachcomputer"
                            Grid.Row="4"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Sprachcomputer" />
                                <Grid
                                    x:Name="KDKT8"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,*,*">
                                    <Grid
                                        Grid.Column="0"
                                        Margin="5,10,5,0"
                                        ColumnDefinitions="Auto,*">
                                        <CheckBox
                                            Grid.Column="0"
                                            MinWidth="0"
                                            VerticalAlignment="Bottom"
                                            IsChecked="{x:Bind ViewModel.ParamterDictionary['var_BenDef_24'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                        <TextBox
                                            Grid.Column="1"
                                            PlaceholderText="benutzerdefinierte Eingabe"
                                            Text="{x:Bind ViewModel.ParamterDictionary['var_BenDef_24'].Comment, Mode=TwoWay, Converter={StaticResource CustomInputStringConveter}}" />
                                    </Grid>
                                    <CheckBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="in Lieferung enthalten"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabinLiefenth'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="kundenseitig"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabkundenseitig'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Leuchtanzeige"
                            Grid.Row="5"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="4"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="3"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Leuchtanzeige" />
                                <Grid
                                    x:Name="KDKT9"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="3*,*">
                                    <CheckBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Multifunktionsanzeige inklusive TKS / Überlast / Summer / Notlicht"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabMultifunktionsanzeigeinklTKSueLSumNL'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Überlast inklusive Summer"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabueberlastinklSummer'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                </Grid>
                                <Grid
                                    x:Name="KDKT10"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*,*,*">
                                    <CheckBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Beschriftungsfeld inklusive TKS / Notlicht"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabBeschriftfeldinclTKSNL'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Evakuierung"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabEvakuierung'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Brandfall"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabBrandfall'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="3"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Feuerwehrfahrt"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabFeuerwfahrt'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                </Grid>
                                <Grid
                                    x:Name="KDKT11"
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*,2*">
                                    <CheckBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Anzeige über Standanzeige"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabAnzeigeueberStandanzeige'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="Außer Betrieb"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabAussBetrieb'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <Grid
                                        Grid.Column="2"
                                        Margin="5,10,5,0"
                                        ColumnDefinitions="Auto,*">
                                        <CheckBox
                                            Grid.Column="0"
                                            MinWidth="0"
                                            VerticalAlignment="Bottom"
                                            IsChecked="{x:Bind ViewModel.ParamterDictionary['var_BenDef_25'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                        <TextBox
                                            Grid.Column="1"
                                            PlaceholderText="benutzerdefinierte Eingabe"
                                            Text="{x:Bind ViewModel.ParamterDictionary['var_BenDef_25'].Comment, Mode=TwoWay, Converter={StaticResource CustomInputStringConveter}}" />
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Vandalenklasse"
                            Grid.Row="6"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Vandalenklasse" />
                                <Grid
                                    x:Name="KDKT12"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*,*,*">
                                    <CheckBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="0"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabFTVandalenklasse0'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="1"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabFTVandalenklasse1'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <CheckBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="2"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabFTVandalenklasse2'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                    <Grid
                                        Grid.Column="3"
                                        Margin="5,10,5,0"
                                        ColumnDefinitions="Auto,*">
                                        <CheckBox
                                            Grid.Column="0"
                                            MinWidth="0"
                                            VerticalAlignment="Bottom"
                                            IsChecked="{x:Bind ViewModel.ParamterDictionary['var_BenDef_26'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                        <TextBox
                                            Grid.Column="1"
                                            PlaceholderText="benutzerdefinierte Eingabe"
                                            Text="{x:Bind ViewModel.ParamterDictionary['var_BenDef_26'].Comment, Mode=TwoWay, Converter={StaticResource CustomInputStringConveter}}" />
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Tasterprogramm"
                            Grid.Row="7"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Tasterprogramm" />
                                <Grid
                                    x:Name="KDKT13"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,3*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        Header="Typ"
                                        LiftParameter="{x:Bind ViewModel.ParamterDictionary['var_KabTabTaster'], Mode=TwoWay}" />
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        Header="Zusatzinformationen"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_Taster_Info'].Value, Mode=TwoWay}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Tasterplatten"
                            Grid.Row="8"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Tasterplatten" />
                                <Grid
                                    x:Name="KDKT14"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,2*,*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        Header="Typ"
                                        LiftParameter="{x:Bind ViewModel.ParamterDictionary['var_KabTabTasterplatten'], Mode=TwoWay}" />
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="1"
                                        Header="Material"
                                        LiftParameter="{x:Bind ViewModel.ParamterDictionary['var_KabTabTasterplattenmaterial'], Mode=TwoWay}" />
                                    <CheckBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        Content="akustische Quittung"
                                        IsChecked="{x:Bind ViewModel.ParamterDictionary['var_KabTabakustischeQuittung'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="FarbeLEDQuittung"
                            Grid.Row="9"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Farbe LED-Quittung" />
                                <Grid
