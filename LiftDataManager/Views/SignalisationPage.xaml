                                        Header="Aufbau"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KabTabAufbau'], Mode=TwoWay}" />
                                    <SpeziControls:ParameterTextBox
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_FT_Info2'], Mode=TwoWay}"
