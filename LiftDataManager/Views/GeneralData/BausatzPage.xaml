<Page
    x:Class="LiftDataManager.Views.BausatzPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:SpeziControls="using:LiftDataManager.Controls"
    xmlns:converters="using:CommunityToolkit.WinUI.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:LiftDataManager.Helpers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:media="using:CommunityToolkit.WinUI.Media"
    xmlns:ui="using:CommunityToolkit.WinUI"
    mc:Ignorable="d">

    <Page.Resources>
        <media:AttachedCardShadow
            x:Key="CommonShadow"
            CornerRadius="5"
            InnerContentClipMode="CompositionMaskBrush"
            Offset="5" />
        <converters:BoolToObjectConverter
            x:Key="InvertBoolToObjectConverter"
            FalseValue="Visible"
            TrueValue="Collapsed" />
        <helpers:NewLineConveter x:Key="NewLineConveter" />
        <helpers:CustomInputStringConveter x:Key="CustomInputStringConveter" />
        <helpers:StringToBoolConverter x:Key="StringToBoolConverter" />
        <helpers:StringToVisibilityConverter x:Key="StringToVisibilityConverter" />
    </Page.Resources>
    <Grid x:Name="ContentArea">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <!--  Parameter  -->
        <ScrollViewer
            Grid.Row="0"
            Margin="0,0,0,0"
            Padding="0,0,15,0">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Expander
                    x:Name="Bausatzdaten"
                    Grid.Row="0"
                    Margin="0,0,0,10"
                    Header="Bausatzdaten"
                    IsExpanded="True"
                    Style="{StaticResource ExpanderOnlyOpenContentStyle}">
                    <Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,*">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="2*" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <Grid x:Name="Bausatztyp" Grid.Row="0">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="4"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="3"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Bausatztyp" />
                                <Grid
                                    x:Name="Bausatz0"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,auto,auto,auto">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Bausatz'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                    <Border
                                        Grid.Column="1"
                                        Height="34"
                                        Margin="0,0,5,0"
                                        VerticalAlignment="Bottom"
                                        BorderBrush="YellowGreen"
                                        BorderThickness="2"
                                        CornerRadius="4"
                                        Visibility="{x:Bind ViewModel.IsCFPFrame, Mode=OneWay}">
                                        <StackPanel Orientation="Horizontal">
                                            <FontIcon
                                                Margin="5,5,0,5"
                                                VerticalAlignment="Center"
                                                Glyph="&#xEDB1;" />
                                            <TextBlock
                                                Margin="5"
                                                VerticalAlignment="Center"
                                                Text="CFP - Bausatz" />
                                        </StackPanel>
                                        <Border.Background>
                                            <SolidColorBrush Opacity="0.2" Color="YellowGreen" />
                                        </Border.Background>
                                        <ToolTipService.ToolTip>
                                            <ToolTip>
                                                <TextBlock Text="{x:Bind ViewModel.CFPFrameInfoToolTip, Mode=OneWay}" />
                                            </ToolTip>
                                        </ToolTipService.ToolTip>
                                    </Border>
                                    <FontIcon
                                        Grid.Column="2"
                                        Margin="5"
                                        VerticalAlignment="Bottom"
                                        Foreground="Orange"
                                        Glyph="&#xE814;"
                                        Visibility="{x:Bind ViewModel.IsCFPDataBaseOverwritten, Mode=OneWay}">
                                        <ToolTipService.ToolTip>
                                            <ToolTip>
                                                <StackPanel>
                                                    <TextBlock
                                                        FontSize="14"
                                                        FontWeight="Bold"
                                                        Foreground="Orange"
                                                        Text="Achtung:" />
                                                    <TextBlock Foreground="Orange" Text="Dieser Bausatz enthält Datenbankanpassungen!" />
                                                </StackPanel>
                                            </ToolTip>
                                        </ToolTipService.ToolTip>
                                    </FontIcon>
                                    <TextBox
                                        Grid.Column="3"
                                        Margin="0,0,5,0"
                                        Header="Gewicht [kg]"
                                        IsReadOnly="True"
                                        Text="{x:Bind ViewModel.CarSlingWeight, Mode=OneWay}" />
                                </Grid>
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="5,0,0,0"
                                    Foreground="GreenYellow"
                                    TextWrapping="WrapWholeWords"
                                    Visibility="{x:Bind ViewModel.ShowCFPFrameInfo, Mode=OneWay}">
                                    <Run
                                        FontSize="16"
                                        FontWeight="Bold"
                                        Text="Information:" />
                                    <Run FontSize="14" Text=" Bausatz wurde noch nicht im Car Frame Programm angelegt!" />
                                </TextBlock>
                                <SpeziControls:ParameterTextBox
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Bausatz_ZT'], Mode=TwoWay}"
                                    ShowDefaultHeader="True" />
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="FuehrungFK"
                            Grid.Row="1"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Führungsart FK" />
                                <Grid
                                    x:Name="Bausatz1"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Fuehrungsart'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TypFuehrung'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="FuehrungGGW"
                            Grid.Row="2"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="{x:Bind ViewModel.CWTGuideName, Mode=OneWay}" />
                                <Grid
                                    x:Name="Bausatz2"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        Header="{x:Bind ViewModel.CWTGuideName, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Fuehrungsart_GGW'], Mode=TwoWay}" />
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="1"
                                        Header="{x:Bind ViewModel.CWTGuideTyp, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TypFuehrung_GGW'], Mode=TwoWay}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="SchieneFK"
                            Grid.Row="3"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Führungsschienen FK" />
                                <Grid
                                    x:Name="Bausatz3"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_FuehrungsschieneFahrkorb'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_StatusFuehrungsschienen'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="SchieneGGW"
                            Grid.Row="4"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="{x:Bind ViewModel.CWTRailName, Mode=OneWay}" />
                                <Grid
                                    x:Name="Bausatz4"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        Header="{x:Bind ViewModel.CWTRailName, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_FuehrungsschieneGegengewicht'], Mode=TwoWay}" />
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="1"
                                        Header="{x:Bind ViewModel.CWTRailState, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_StatusGGWSchienen'], Mode=TwoWay}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Fangvorrichtung"
                            Grid.Row="5"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto, Auto, 10">
                                <Border
                                    Grid.RowSpan="3"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="2"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Fangvorrichtung" />
                                <Grid
                                    x:Name="Bausatz5"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Fangvorrichtung'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TypFV'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                </Grid>
                                <Grid
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Grid.ColumnSpan="2"
                                    Margin="5,0,5,0"
                                    RowDefinitions="Auto, Auto">
                                    <TextBlock Text="Fangvorrichtungsbereich" />
                                    <TextBlock Grid.Row="1" Text="{x:Bind ViewModel.Safetygearworkarea, Mode=OneWay}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="GB"
                            Grid.Row="6"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="4"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="3"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16">
                                    Geschwindigkeits-<LineBreak />
                                    begrenzer</TextBlock>
                                <SpeziControls:ParameterComboBox
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Geschwindigkeitsbegrenzer'], Mode=TwoWay}" />
                                <SpeziControls:ParameterTextBox
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Geschwindigkeitsbeg_ZT'], Mode=TwoWay}"
                                    ShowDefaultHeader="True" />
                                <SpeziControls:ParameterComboBox
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SpanngewichtTyp'], Mode=TwoWay}"
                                    ShowDefaultHeader="True"
                                    Visibility="{x:Bind ViewModel.OverspeedGovernorWeightVisibility, Mode=OneWay}" />
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Ersatzmassnahmen"
                            Grid.Row="7"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="3"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="2"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Ersatzmaßnahmen" />
                                <SpeziControls:ParameterComboBox
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Ersatzmassnahmen'], Mode=TwoWay}" />
                                <SpeziControls:ParameterTextBox
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Ersatzmaßnahmen_ZT'], Mode=TwoWay}"
                                    ShowDefaultHeader="True" />
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Schachtinformation"
                            Grid.Row="8"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="3"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="2"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Schachtinformation" />
                                <SpeziControls:ParameterComboBox
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Schachtinformationssystem'], Mode=TwoWay}" />
                                <SpeziControls:ParameterTextBox
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Schachtinformation_ZT'], Mode=TwoWay}"
                                    ShowDefaultHeader="True" />
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Lastmessung"
                            Grid.Row="9"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="3"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="2"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16">
                                    Lastmesseinrichtung/<LineBreak />
                                    Aufsetzvorrichtung</TextBlock>
                                <Grid
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    ColumnDefinitions="*,Auto,Auto">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        Margin="0,0,5,0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Lastmesseinrichtung'], Mode=TwoWay}"
                                        ShowDefaultHeader="False" />
                                    <SpeziControls:ParameterCheckBox
                                        x:Name="Aufsetzvorrichtung"
                                        Grid.Column="1"
                                        Margin="0,0,5,0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Aufsetzvorrichtung'], Mode=TwoWay}"
                                        ShowDefaultCheckBoxContent="True" />
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="3"
                                        Margin="0,0,5,0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_AufsetzvorrichtungSystem'], Mode=TwoWay}"
                                        ShowDefaultHeader="False"
                                        Visibility="{x:Bind ViewModel.ParameterDictionary['var_Aufsetzvorrichtung'].Value, Mode=OneWay, Converter={StaticResource StringToVisibilityConverter}}" />
                                </Grid>
                                <SpeziControls:ParameterTextBox
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Lastmesseinrichtung_ZT'], Mode=TwoWay}"
                                    ShowDefaultHeader="True" />
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Beschichtung"
                            Grid.Row="10"
                            Grid.ColumnSpan="2"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="4"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.RowSpan="3"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Beschichtung Bausatz" />
                                <Grid
                                    x:Name="Bausatz10"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,*">
                                    <SpeziControls:ParameterComboBox LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Beschichtung'], Mode=TwoWay}" ShowDefaultHeader="True" />
                                    <SpeziControls:ParameterTextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_RALTonTragrahmen'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                </Grid>
                                <Grid
                                    x:Name="Bausatz11"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,*">
                                    <SpeziControls:ParameterComboBox LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_BeschichtungSchachteinbauten'], Mode=TwoWay}" ShowDefaultHeader="True" />
                                    <SpeziControls:ParameterTextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_RALTonSchachteinbauten'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                </Grid>
                                <Grid
                                    x:Name="Bausatz12"
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,*">
                                    <SpeziControls:ParameterComboBox LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_BeschichtungAntrieb'], Mode=TwoWay}" ShowDefaultHeader="True" />
                                    <SpeziControls:ParameterTextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_RALTonAntrieb'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="SonstigesBausatz"
                            Grid.Row="11"
                            Grid.ColumnSpan="2"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Sonstiges Bausatz" />
                                <Grid
                                    x:Name="Bausatz13"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*">
                                    <TextBox
                                        Grid.Column="0"
                                        MinHeight="120"
                                        Margin="5,5,5,0"
                                        AcceptsReturn="True"
                                        Text="{x:Bind ViewModel.ParameterDictionary['var_SonstigesBausatz'].Value, Converter={StaticResource NewLineConveter}, Mode=TwoWay}"
                                        TextWrapping="Wrap" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <!--  Datafield CFP Parameter  -->
                        <Border
                            Grid.Row="0"
                            Grid.RowSpan="10"
                            Grid.Column="1"
                            Margin="0,0,5,10"
                            ui:Effects.Shadow="{StaticResource CommonShadow}"
                            Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                        <Grid
                            x:Name="CFPParameter"
                            Grid.Row="0"
                            Grid.RowSpan="10"
                            Grid.Column="1"
                            Margin="15,10,15,10">
                            <StackPanel
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="0,0,0,10"
                                Spacing="1">
                                <TextBlock
                                    FontSize="18"
                                    FontWeight="Medium"
                                    Text="Bausatz Parameter" />
                                <TextBlock
                                    Grid.Row="1"
                                    Margin="0,10,0,5"
                                    FontSize="16"
                                    FontWeight="Medium"
                                    Text="Basisdaten" />
                                <Grid Grid.ColumnDefinitions="auto,*,30">
                                    <TextBlock VerticalAlignment="Center" Text="Stichmaß:" />
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="0,0,5,0"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        Text="{x:Bind ViewModel.ParameterDictionary['var_Stichmass'].Value, Mode=OneWay}" />
                                    <TextBlock
                                        Grid.Column="2"
                                        VerticalAlignment="Center"
                                        Text="mm" />
                                </Grid>
                                <Grid Grid.ColumnDefinitions="auto,*,30">
                                    <TextBlock VerticalAlignment="Center" Text="Unterfahrt bis Puffer:" />
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="0,0,5,0"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        Text="{x:Bind ViewModel.ParameterDictionary['var_FUBP'].Value, Mode=OneWay}" />
                                    <TextBlock
                                        Grid.Column="2"
                                        VerticalAlignment="Center"
                                        Text="mm" />
                                </Grid>
                                <Grid Grid.ColumnDefinitions="auto,*,30">
                                    <TextBlock VerticalAlignment="Center" Text="Ünterfahrt mit Pufferhub:" />
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="0,0,5,0"
                                        HorizontalAlignment="Right"
                                        VerticalAlignment="Center"
                                        Text="{x:Bind ViewModel.ParameterDictionary['var_RHO'].Value, Mode=OneWay}" />
