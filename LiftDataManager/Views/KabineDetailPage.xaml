<Page
    x:Class="LiftDataManager.Views.KabineDetailPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:SpeziControls="using:LiftDataManager.Controls"
    xmlns:converters="using:CommunityToolkit.WinUI.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:LiftDataManager.Helpers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:media="using:CommunityToolkit.WinUI.Media"
    xmlns:ui="using:CommunityToolkit.WinUI"
    mc:Ignorable="d">

    <Page.Resources>
        <media:AttachedCardShadow
            x:Key="CommonShadow"
            CornerRadius="5"
            Offset="5" />
        <converters:BoolToObjectConverter
            x:Key="InvertBoolToObjectConverter"
            FalseValue="Visible"
            TrueValue="Collapsed" />
        <helpers:CustomInputStringConveter x:Key="CustomInputStringConveter" />
        <helpers:StringToBoolConverter x:Key="StringToBoolConverter" />
        <helpers:DoubleToStringConverter x:Key="DoubleToStringConverter" />
    </Page.Resources>
    <Grid x:Name="ContentArea">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <ScrollViewer
            Grid.Row="0"
            Margin="0,0,0,0"
            Padding="0,0,15,0">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Expander
                    Grid.Row="0"
                    Margin="0,0,0,10"
                    Header="Detaildaten Kabine"
                    IsExpanded="True"
                    Style="{StaticResource ExpanderContentStyle}">
                    <Pivot SelectionChanged="Pivot_SelectionChanged">
                        <Pivot.Items>
                            <PivotItem>
                                <PivotItem.Header>
                                    <TextBlock Style="{StaticResource NavigationViewItemHeaderTextStyle}" Text="Allgemeine Daten" />
                                </PivotItem.Header>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <TextBlock
                                        Grid.Row="0"
                                        Grid.Column="0"
                                        Margin="0,5,0,0"
                                        VerticalAlignment="Top"
                                        FontSize="24"
                                        FontWeight="Bold"
                                        Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}"
                                        Text="Grundabmessungen" />
                                    <SpeziControls:ParameterCheckBox
                                        Grid.Row="0"
                                        Grid.Column="1"
                                        Margin="-5,10,0,5"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_abgDecke'], Mode=TwoWay}"
                                        ShowDefaultCheckBoxContent="True" />
                                    <Image
                                        Grid.Row="0"
                                        Grid.RowSpan="5"
                                        Grid.Column="0"
                                        Height="275"
                                        Source="/Images/CarDetails/carCeilingDetail.png" />
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_DeckenCSchienenHoehe'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ShowDefaultHeader="True"
                                        Unit="mm" />
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Row="2"
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_GrundDeckenhoehe'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ReadOnly="True"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <TextBlock>
                                                <Run FontSize="16" Text="Schreibgeschützer Parameter:" /><LineBreak />
                                                Hauptdeckenhöhe wird automatisch gesetzt<LineBreak />
                                                - Kabinenbreite größer 2000 (120 mm)<LineBreak />
                                                - Kabinenbreite größer 1400 (85 mm)<LineBreak />
                                                - Kabinenbreite kleiner gleich 1400 (50 mm)<LineBreak />
                                                bei Bedarf kann die Deckenhöhe überschieben werden</TextBlock>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Row="3"
                                        Grid.Column="1"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_abgDecke'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_abgeDeckeHoehe'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ReadOnly="True"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <TextBlock>
                                                <Run FontSize="16" Text="Schreibgeschützer Parameter:" /><LineBreak />
                                                abgehängte Deckenhöhe wird automatisch gesetzt<LineBreak />
                                                - Standardhöhe abgehängte Decke (35 mm)<LineBreak />
                                                bei Bedarf kann die Deckenhöhe überschieben werden</TextBlock>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Row="4"
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KHRoh'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ReadOnly="True"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <TextBlock>
                                                <Run FontSize="16" Text="Schreibgeschützer Parameter:" /><LineBreak />
                                                Kabinenhöhe roh wird automatisch gesetzt<LineBreak />
                                                Kabinenhöhe roh = OK-FFB zu UK-Hauptdecke</TextBlock>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Row="1"
                                        Grid.Column="2"
                                        Header="Kabinendeckenhöhe gesamt"
                                        IsEnabled="False"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KD'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ShowDefaultHeader="False"
                                        Unit="mm" />
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Row="2"
                                        Grid.Column="2"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_overrideDefaultCeiling'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ShowDefaultHeader="True"
                                        Unit="mm" />
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Row="3"
                                        Grid.Column="2"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_abgDecke'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_overrideSuspendedCeiling'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ShowDefaultHeader="True"
                                        Unit="mm" />
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Row="4"
                                        Grid.Column="2"
                                        IsEnabled="False"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KHLicht'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ShowDefaultHeader="True"
                                        Unit="mm" />
                                    <TextBlock
                                        Grid.Row="5"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="2"
                                        Margin="5,10,5,5"
                                        Foreground="IndianRed"
                                        TextWrapping="WrapWholeWords">
                                        <Run
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Achtung:" />
                                        <Run FontSize="14" Text="Automatische Kabinendeckenvalidierung erst ab einem Erstellungsdatum der Spezifikation nach dem 10.01.2024 aktiv." />
                                    </TextBlock>

                                </Grid>
                            </PivotItem>
                            <PivotItem x:Name="CarEquipment">
                                <PivotItem.Header>
                                    <TextBlock Style="{StaticResource NavigationViewItemHeaderTextStyle}" Text="Kabinenausstattung" />
                                </PivotItem.Header>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="250" />
                                        <RowDefinition Height="250" />
                                        <RowDefinition Height="250" />
                                        <RowDefinition Height="250" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <SpeziControls:CarEquipmentControl
                                        x:Name="CarEquipmentControlA"
                                        Grid.Row="0"
                                        Grid.Column="0"
                                        ItemSource="{x:Bind ViewModel.ParameterDictionary, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        Side="A" />
                                    <SpeziControls:CarEquipmentControl
                                        x:Name="CarEquipmentControlB"
                                        Grid.Row="1"
                                        Grid.Column="0"
                                        ItemSource="{x:Bind ViewModel.ParameterDictionary, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        Side="B" />
                                    <SpeziControls:CarEquipmentControl
                                        x:Name="CarEquipmentControlC"
                                        Grid.Row="2"
                                        Grid.Column="0"
                                        ItemSource="{x:Bind ViewModel.ParameterDictionary, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        Side="C" />
                                    <SpeziControls:CarEquipmentControl
                                        x:Name="CarEquipmentControlD"
                                        Grid.Row="3"
                                        Grid.Column="0"
                                        ItemSource="{x:Bind ViewModel.ParameterDictionary, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                        Side="D" />
                                    <Grid
                                        Grid.Row="0"
                                        Grid.RowSpan="5"
                                        Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="200" />
                                            <ColumnDefinition />
                                            <ColumnDefinition Width="35" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>
                                        <TextBlock
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3"
                                            Margin="0,5,0,5"
                                            VerticalAlignment="Top"
                                            FontSize="24"
                                            FontWeight="Bold"
                                            Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}"
                                            Text="Grundabmessungen" />
                                        <TextBlock
                                            Grid.Row="1"
                                            Grid.Column="0"
                                            VerticalAlignment="Center"
                                            Text="Kabinenbreite" />
                                        <TextBox
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Margin="5,5,5,0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Center"
                                            IsReadOnly="True"
                                            Text="{x:Bind ViewModel.ParameterDictionary['var_KBI'].Value}"
                                            TextAlignment="Right" />
                                        <TextBlock
                                            Grid.Row="1"
                                            Grid.Column="2"
                                            VerticalAlignment="Center"
                                            Text="mm" />
                                        <TextBlock
                                            Grid.Row="2"
                                            Grid.Column="0"
                                            VerticalAlignment="Center"
                                            Text="Kabinentiefe" />
                                        <TextBox
                                            Grid.Row="2"
                                            Grid.Column="1"
                                            Margin="5,5,5,0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Center"
                                            IsReadOnly="True"
                                            Text="{x:Bind ViewModel.ParameterDictionary['var_KTI'].Value}"
                                            TextAlignment="Right" />
                                        <TextBlock
                                            Grid.Row="2"
                                            Grid.Column="2"
                                            VerticalAlignment="Center"
                                            Text="mm" />
                                        <TextBlock
                                            Grid.Row="3"
                                            Grid.Column="0"
                                            VerticalAlignment="Center"
                                            Text="Kabinenhöhe (roh)" />
                                        <TextBox
                                            Grid.Row="3"
                                            Grid.Column="1"
                                            Margin="5,5,5,0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Center"
                                            IsReadOnly="True"
                                            Text="{x:Bind ViewModel.ParameterDictionary['var_KHRoh'].Value}"
                                            TextAlignment="Right" />
                                        <TextBlock
                                            Grid.Row="3"
                                            Grid.Column="2"
                                            VerticalAlignment="Center"
                                            Text="mm" />
                                        <TextBlock
                                            Grid.Row="4"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3"
                                            Margin="0,5,0,5"
                                            VerticalAlignment="Top"
                                            FontSize="18"
                                            FontWeight="Bold"
                                            Text="Türdaten" />
                                        <TextBlock
                                            Grid.Row="5"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3">
                                            <Run
                                                FontSize="16"
                                                FontWeight="Bold"
                                                Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}"
                                                Text="Zugang A  " />
                                            <Run FontWeight="Bold" Text="TürTyp: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_Tuerbezeichnung'].Value}" />
                                            <Run FontWeight="Bold" Text="Türbreite: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_TB'].Value}" />
                                            <Run Text=" mm" />
                                            <Run FontWeight="Bold" Text="Türhöhe: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_TH'].Value}" />
                                            <Run Text=" mm " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_Tueroeffnung'].Value}" />
                                        </TextBlock>
                                        <TextBlock
                                            Grid.Row="6"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3">
                                            <Run
                                                FontSize="16"
                                                FontWeight="Bold"
                                                Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}"
                                                Text="Zugang B  " />
                                            <Run FontWeight="Bold" Text="TürTyp: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_Tuerbezeichnung_B'].Value}" />
                                            <Run FontWeight="Bold" Text="Türbreite: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_TB_B'].Value}" />
                                            <Run Text=" mm" />
                                            <Run FontWeight="Bold" Text="Türhöhe: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_TH_B'].Value}" />
                                            <Run Text=" mm " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_Tueroeffnung_B'].Value}" />
                                        </TextBlock>
                                        <TextBlock
                                            Grid.Row="7"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3">
                                            <Run
                                                FontSize="16"
                                                FontWeight="Bold"
                                                Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}"
                                                Text="Zugang C  " />
                                            <Run FontWeight="Bold" Text="TürTyp: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_Tuerbezeichnung_C'].Value}" />
                                            <Run FontWeight="Bold" Text="Türbreite: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_TB_C'].Value}" />
                                            <Run Text=" mm" />
                                            <Run FontWeight="Bold" Text="Türhöhe: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_TH_C'].Value}" />
                                            <Run Text=" mm " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_Tueroeffnung_C'].Value}" />
                                        </TextBlock>
                                        <TextBlock
                                            Grid.Row="8"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3">
                                            <Run
                                                FontSize="16"
                                                FontWeight="Bold"
                                                Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}"
                                                Text="Zugang D  " />
                                            <Run FontWeight="Bold" Text="TürTyp: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_Tuerbezeichnung_D'].Value}" />
                                            <Run FontWeight="Bold" Text="Türbreite: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_TB_D'].Value}" />
                                            <Run Text=" mm" />
                                            <Run FontWeight="Bold" Text="Türhöhe: " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_TH_D'].Value}" />
                                            <Run Text=" mm " />
                                            <Run Text="{x:Bind ViewModel.ParameterDictionary['var_Tueroeffnung_D'].Value}" />
                                        </TextBlock>
                                        <TextBlock
                                            Grid.Row="9"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3"
                                            Margin="0,5,0,5"
                                            VerticalAlignment="Top"
                                            FontSize="18"
                                            FontWeight="Bold"
                                            Text="Spiegel Detaileingabe" />
                                        <Grid
                                            Grid.Row="10"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition />
                                                <ColumnDefinition />
                                                <ColumnDefinition />
                                                <ColumnDefinition />
                                            </Grid.ColumnDefinitions>
                                            <CheckBox
                                                Grid.Column="0"
                                                Content="Spiegel A"
                                                IsChecked="{x:Bind ViewModel.ParameterDictionary['var_SpiegelA'].Value, Converter={StaticResource StringToBoolConverter}}"
                                                IsHitTestVisible="False" />
                                            <CheckBox
                                                Grid.Column="1"
                                                Content="Spiegel B"
                                                IsChecked="{x:Bind ViewModel.ParameterDictionary['var_SpiegelB'].Value, Converter={StaticResource StringToBoolConverter}}"
                                                IsHitTestVisible="False" />
                                            <CheckBox
                                                Grid.Column="2"
                                                Content="Spiegel C"
                                                IsChecked="{x:Bind ViewModel.ParameterDictionary['var_SpiegelC'].Value, Converter={StaticResource StringToBoolConverter}}"
                                                IsHitTestVisible="False" />
                                            <CheckBox
                                                Grid.Column="3"
                                                Content="Spiegel D"
                                                IsChecked="{x:Bind ViewModel.ParameterDictionary['var_SpiegelD'].Value, Converter={StaticResource StringToBoolConverter}}"
                                                IsHitTestVisible="False" />
                                        </Grid>
                                        <Grid
                                            Grid.Row="11"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="0"
                                                Grid.Column="0"
                                                Header="{x:Bind ViewModel.MirrorDimensionsWidth1, Mode=OneWay}"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_BreiteSpiegel'], Mode=TwoWay}"
                                                ShowDefaultHeader="False"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="0"
                                                Grid.Column="1"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_BreiteSpiegel'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="0"
                                                Grid.Column="2"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_AbstandSpiegelvonLinks'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="1"
                                                Grid.Column="0"
                                                Header="{x:Bind ViewModel.MirrorDimensionsHeight1, Mode=OneWay}"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_HoeheSpiegel'], Mode=TwoWay}"
                                                ShowDefaultHeader="False"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_HoeheSpiegel'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="1"
                                                Grid.Column="2"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_AbstandSpiegelDecke'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                        </Grid>
                                        <Grid
                                            Grid.Row="12"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3"
                                            Visibility="{x:Bind ViewModel.ShowMirrorDimensions2, Mode=OneWay}">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="0"
                                                Grid.Column="0"
                                                Header="{x:Bind ViewModel.MirrorDimensionsWidth2, Mode=OneWay}"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_BreiteSpiegel2'], Mode=TwoWay}"
                                                ShowDefaultHeader="False"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="0"
                                                Grid.Column="1"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_BreiteSpiegel2'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="0"
                                                Grid.Column="2"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_AbstandSpiegelvonLinks2'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="1"
                                                Grid.Column="0"
                                                Header="{x:Bind ViewModel.MirrorDimensionsHeight2, Mode=OneWay}"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_HoeheSpiegel2'], Mode=TwoWay}"
                                                ShowDefaultHeader="False"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_HoeheSpiegel2'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="1"
                                                Grid.Column="2"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_AbstandSpiegelDecke2'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                        </Grid>
                                        <Grid
                                            Grid.Row="13"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="3"
                                            Visibility="{x:Bind ViewModel.ShowMirrorDimensions3, Mode=OneWay}">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="0"
                                                Grid.Column="0"
                                                Header="{x:Bind ViewModel.MirrorDimensionsWidth3, Mode=OneWay}"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_BreiteSpiegel3'], Mode=TwoWay}"
                                                ShowDefaultHeader="False"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="0"
                                                Grid.Column="1"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_BreiteSpiegel3'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="0"
                                                Grid.Column="2"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_AbstandSpiegelvonLinks3'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="1"
                                                Grid.Column="0"
                                                Header="{x:Bind ViewModel.MirrorDimensionsHeight3, Mode=OneWay}"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_HoeheSpiegel3'], Mode=TwoWay}"
                                                ShowDefaultHeader="False"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_HoeheSpiegel3'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                            <SpeziControls:ParameterNumberTextBox
                                                Grid.Row="1"
                                                Grid.Column="2"
                                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_AbstandSpiegelDecke3'], Mode=TwoWay}"
                                                ShowDefaultHeader="True"
                                                Unit="mm" />
                                        </Grid>
                                    </Grid>
                                </Grid>
                            </PivotItem>
                            <PivotItem>
                                <PivotItem.Header>
                                    <TextBlock Style="{StaticResource NavigationViewItemHeaderTextStyle}" Text="Kabinenboden" />
                                </PivotItem.Header>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto" />
                                        <RowDefinition Height="auto" />
                                        <RowDefinition Height="auto" />
                                        <RowDefinition Height="350" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Grid
                                        Grid.Row="0"
