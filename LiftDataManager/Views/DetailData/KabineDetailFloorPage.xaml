<?xml version="1.0" encoding="utf-8" ?>
<Page
    x:Class="LiftDataManager.Views.KabineDetailFloorPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:SpeziControls="using:LiftDataManager.Controls"
    xmlns:converters="using:CommunityToolkit.WinUI.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:LiftDataManager.Helpers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="using:CommunityToolkit.WinUI"
    mc:Ignorable="d">

    <Page.Resources>
        <helpers:StringToBoolConverter x:Key="StringToBoolConverter" />
        <helpers:DoubleToStringConverter x:Key="DoubleToStringConverter" />
        <converters:BoolToObjectConverter
            x:Key="InvertBoolToObjectConverter"
            FalseValue="Visible"
            TrueValue="Collapsed" />
    </Page.Resources>

    <Grid x:Name="ContentArea">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <Expander
            Grid.Row="0"
            Margin="0,0,0,10"
            VerticalAlignment="Top"
            Header="Detaildaten Kabine"
            IsExpanded="True"
            Style="{StaticResource ExpanderContentStyle}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Pivot
                    Grid.Row="0"
                    Margin="0,0,0,10"
                    SelectedIndex="2"
                    SelectedItem="{x:Bind ViewModel.SelectedPivotItem, Mode=TwoWay}">
                    <Pivot.Items>
                        <PivotItem Tag="KabineDetailPage">
                            <PivotItem.Header>
                                <TextBlock Style="{StaticResource NavigationViewItemHeaderTextStyle}" Text="Allgemeine Kabinendaten" />
                            </PivotItem.Header>
                        </PivotItem>
                        <PivotItem Tag="KabineDetailEquipmentPage">
                            <PivotItem.Header>
                                <TextBlock Style="{StaticResource NavigationViewItemHeaderTextStyle}" Text="Kabinenausstattung" />
                            </PivotItem.Header>
                        </PivotItem>
                        <PivotItem Tag="KabineDetailFloorPage">
                            <PivotItem.Header>
                                <TextBlock Style="{StaticResource NavigationViewItemHeaderTextStyle}" Text="Kabinenboden" />
                            </PivotItem.Header>
                        </PivotItem>
                        <PivotItem Tag="KabineDetailLayoutPage">
                            <PivotItem.Header>
                                <TextBlock Style="{StaticResource NavigationViewItemHeaderTextStyle}" Text="Schottengrundriss" />
                            </PivotItem.Header>
                        </PivotItem>
                        <PivotItem Tag="KabineDetailCeilingPage">
                            <PivotItem.Header>
                                <TextBlock Style="{StaticResource NavigationViewItemHeaderTextStyle}" Text="Kabinendecke" />
                            </PivotItem.Header>
                        </PivotItem>
                        <PivotItem Tag="KabineDetailControlPanelPage">
                            <PivotItem.Header>
                                <TextBlock Style="{StaticResource NavigationViewItemHeaderTextStyle}" Text="Kabinentableau" />
                            </PivotItem.Header>
                        </PivotItem>
                    </Pivot.Items>
                </Pivot>
                <ScrollViewer Grid.Row="1">
                    <Grid>
                        <Grid.Resources>
                            <ResourceDictionary Source="ms-appx:///Microsoft.UI.Xaml/DensityStyles/Compact.xaml" />
                        </Grid.Resources>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="350" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Grid
                            Grid.Row="0"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="0,15,0,15">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <CheckBox
                                x:Name="chkboxZugangA"
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Bottom"
                                Content="Zugang A"
                                IsChecked="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_A'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                IsHitTestVisible="False" />
                            <CheckBox
                                x:Name="chkboxZugangB"
                                Grid.Row="1"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Bottom"
                                Content="Zugang B"
                                IsChecked="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_B'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                IsHitTestVisible="False" />
                            <CheckBox
                                x:Name="chkboxZugangC"
                                Grid.Row="2"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Bottom"
                                Content="Zugang C"
                                IsChecked="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_C'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                IsHitTestVisible="False" />
                            <CheckBox
                                x:Name="chkboxZugangD"
                                Grid.Row="3"
                                Grid.Column="0"
                                Margin="5,0,0,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Bottom"
                                Content="Zugang D"
                                IsChecked="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_D'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                IsHitTestVisible="False" />
                            <SpeziControls:ParameterTextBox
                                Grid.Row="0"
                                Grid.Column="1"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Tuerbezeichnung'], Mode=OneWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterTextBox
                                Grid.Row="1"
                                Grid.Column="1"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Tuerbezeichnung_B'], Mode=OneWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterTextBox
                                Grid.Row="2"
                                Grid.Column="1"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Tuerbezeichnung_C'], Mode=OneWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterTextBox
                                Grid.Row="3"
                                Grid.Column="1"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Tuerbezeichnung_D'], Mode=OneWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterTextBox
                                Grid.Row="0"
                                Grid.Column="2"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TB'], Mode=OneWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterTextBox
                                Grid.Row="1"
                                Grid.Column="2"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TB_B'], Mode=OneWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterTextBox
                                Grid.Row="2"
                                Grid.Column="2"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TB_C'], Mode=OneWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterTextBox
                                Grid.Row="3"
                                Grid.Column="2"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TB_D'], Mode=OneWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterComboBox
                                Grid.Row="0"
                                Grid.Column="3"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SchwellenprofilKabTuere'], Mode=TwoWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterComboBox
                                Grid.Row="1"
                                Grid.Column="3"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SchwellenprofilKabTuereB'], Mode=TwoWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterComboBox
                                Grid.Row="2"
                                Grid.Column="3"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SchwellenprofilKabTuereC'], Mode=TwoWay}"
                                ShowDefaultHeader="True" />
                            <SpeziControls:ParameterComboBox
                                Grid.Row="3"
                                Grid.Column="3"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SchwellenprofilKabTuereD'], Mode=TwoWay}"
                                ShowDefaultHeader="True" />
                            <ComboBox
                                Grid.Row="0"
                                Grid.Column="4"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                Header="Türöffnungsrichtung A"
                                ItemsSource="{x:Bind ViewModel.OpeningDirections, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.OpeningDirectionA, Mode=TwoWay}"
                                Visibility="{x:Bind ViewModel.CanEditSillEntranceA, Mode=OneWay}" />
                            <ComboBox
                                Grid.Row="1"
                                Grid.Column="4"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                Header="Türöffnungsrichtung B"
                                ItemsSource="{x:Bind ViewModel.OpeningDirections, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.OpeningDirectionB, Mode=TwoWay}"
                                Visibility="{x:Bind ViewModel.CanEditSillEntranceB, Mode=OneWay}" />
                            <ComboBox
                                Grid.Row="2"
                                Grid.Column="4"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                Header="Türöffnungsrichtung C"
                                ItemsSource="{x:Bind ViewModel.OpeningDirections, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.OpeningDirectionC, Mode=TwoWay}"
                                Visibility="{x:Bind ViewModel.CanEditSillEntranceC, Mode=OneWay}" />
                            <ComboBox
                                Grid.Row="3"
                                Grid.Column="4"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                Header="Türöffnungsrichtung D"
                                ItemsSource="{x:Bind ViewModel.OpeningDirections, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.OpeningDirectionD, Mode=TwoWay}"
                                Visibility="{x:Bind ViewModel.CanEditSillEntranceD, Mode=OneWay}" />
                            <SpeziControls:ParameterComboBox
                                Grid.Row="0"
                                Grid.Column="4"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Tueroeffnung'], Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Visibility="{x:Bind ViewModel.CanEditSillEntranceA, Converter={StaticResource InvertBoolToObjectConverter}, Mode=OneWay}" />
                            <SpeziControls:ParameterComboBox
                                Grid.Row="1"
                                Grid.Column="4"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Tueroeffnung_B'], Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Visibility="{x:Bind ViewModel.CanEditSillEntranceB, Converter={StaticResource InvertBoolToObjectConverter}, Mode=OneWay}" />
                            <SpeziControls:ParameterComboBox
                                Grid.Row="2"
                                Grid.Column="4"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Tueroeffnung_C'], Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Visibility="{x:Bind ViewModel.CanEditSillEntranceC, Converter={StaticResource InvertBoolToObjectConverter}, Mode=OneWay}" />
                            <SpeziControls:ParameterComboBox
                                Grid.Row="3"
                                Grid.Column="4"
                                IsEnabled="False"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Tueroeffnung_D'], Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Visibility="{x:Bind ViewModel.CanEditSillEntranceD, Converter={StaticResource InvertBoolToObjectConverter}, Mode=OneWay}" />
                            <StackPanel
                                Grid.Row="4"
                                Grid.Column="1"
                                Grid.ColumnSpan="4"
                                Margin="5"
                                Orientation="Horizontal"
                                Visibility="{x:Bind ViewModel.OpeningDirectionNotSelected, Mode=OneWay}">
                                <FontIcon
                                    Margin="0,0,5,0"
                                    FontSize="20"
                                    FontWeight="Bold"
                                    Foreground="IndianRed"
                                    Glyph="&#xE7BA;" />
                                <TextBlock
                                    FontSize="16"
                                    FontWeight="Bold"
                                    Foreground="IndianRed"
                                    Text="Öffnungrichtung(en) der einseitigen Tür(en) noch nicht gewählt!" />
                            </StackPanel>
                        </Grid>
                        <SpeziControls:ParameterCheckBox
                            Grid.Row="1"
                            Grid.Column="0"
                            Grid.ColumnSpan="2"
                            Margin="-5,10,0,10"
                            LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_AutogenerateFloorDoorData'], Mode=TwoWay}"
                            ShowDefaultCheckBoxContent="True" />
                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="0"
                            Margin="0,0,0,10"
                            VerticalAlignment="Bottom"
                            FontSize="24"
                            FontWeight="Bold"
                            Foreground="{ThemeResource AccentTextFillColorPrimaryBrush}"
                            Text="Kabinenboden (Ansicht von unten)" />
                        <Viewbox Grid.Row="3" Grid.Column="0">
                            <Rectangle
                                Width="1100"
                                Height="1400"
                                Fill="DarkCyan" />
                        </Viewbox>
                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="1"
                            Margin="0,0,0,10"
                            VerticalAlignment="Bottom"
                            FontSize="16"
                            FontWeight="Bold"
                            Text="Detaileingaben Kabinenboden" />
                        <Grid
                            Grid.Row="4"
                            Grid.Column="0"
                            Margin="0,10,0,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="5,5,5,15"
                                Style="{StaticResource NavigationViewItemHeaderTextStyle}"
                                Text="Kabinenboden" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="1"
                                Grid.Column="0"
                                IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_A'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_EinzugN_A'], Mode=TwoWay}"
                                ReadOnly="{x:Bind ViewModel.ParameterDictionary['var_AutogenerateFloorDoorData'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="1"
                                Grid.Column="1"
                                IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_B'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_EinzugN_B'], Mode=TwoWay}"
                                ReadOnly="{x:Bind ViewModel.ParameterDictionary['var_AutogenerateFloorDoorData'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="2"
                                Grid.Column="0"
                                IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_C'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_EinzugN_C'], Mode=TwoWay}"
                                ReadOnly="{x:Bind ViewModel.ParameterDictionary['var_AutogenerateFloorDoorData'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="2"
                                Grid.Column="1"
                                IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_D'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_EinzugN_D'], Mode=TwoWay}"
                                ReadOnly="{x:Bind ViewModel.ParameterDictionary['var_AutogenerateFloorDoorData'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <Image
                                Grid.Row="3"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Height="200"
                                Margin="0,10,0,10"
                                Source="/Images/CarDetails/connectionAngle.png" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="4"
                                Grid.Column="0"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Anschlusswinkel_x1'], Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="4"
                                Grid.Column="1"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Anschlusswinkel_x2'], Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="5"
                                Grid.Column="0"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Anschlusswinkel_y1'], Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="5"
                                Grid.Column="1"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Anschlusswinkel_y2'], Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                        </Grid>
                        <Grid Grid.Row="4" Grid.Column="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="5,5,5,15"
                                Style="{StaticResource NavigationViewItemHeaderTextStyle}"
                                Text="Schwellenunterbau" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="1"
                                Grid.Column="0"
                                IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_A'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SchwellenbreiteA'], Mode=TwoWay}"
                                ReadOnly="{x:Bind ViewModel.ParameterDictionary['var_AutogenerateFloorDoorData'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="1"
                                Grid.Column="1"
                                IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_B'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SchwellenbreiteB'], Mode=TwoWay}"
                                ReadOnly="{x:Bind ViewModel.ParameterDictionary['var_AutogenerateFloorDoorData'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="2"
                                Grid.Column="0"
                                IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_C'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SchwellenbreiteC'], Mode=TwoWay}"
                                ReadOnly="{x:Bind ViewModel.ParameterDictionary['var_AutogenerateFloorDoorData'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterNumberTextBox
                                Grid.Row="2"
                                Grid.Column="1"
                                IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_D'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SchwellenbreiteD'], Mode=TwoWay}"
                                ReadOnly="{x:Bind ViewModel.ParameterDictionary['var_AutogenerateFloorDoorData'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                ShowDefaultHeader="True"
                                Unit="mm" />
                            <SpeziControls:ParameterComboBox
                                Grid.Row="3"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_SchwellenUnterbauStaerke'], Mode=TwoWay}"
                                ShowDefaultHeader="True" />
                            <Grid
                                Grid.Row="4"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Margin="5,0,5,10">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="330" />
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                    <RowDefinition Height="auto" />
                                </Grid.RowDefinitions>
                                <Button
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="0,5,0,0"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Center"
                                    Command="{x:Bind ViewModel.RefreshCarFloorSillParameterCommand}"
                                    Content="Refresh CarFloor and SillParameter"
                                    Style="{StaticResource AccentButtonStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Text="Zugang A" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="2"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Text="Zugang B" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="3"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Text="Zugang C" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="4"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Text="Zugang D" />
                                <TextBlock
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    VerticalAlignment="Center"
                                    Text="Trägerblech Verlängerung links ab TB" />
                                <TextBlock
                                    Grid.Row="2"
                                    Grid.Column="0"
                                    VerticalAlignment="Center"
                                    Text="Trägerblech Verlängerung rechts ab TB" />
                                <TextBlock
                                    Grid.Row="3"
                                    Grid.Column="0"
                                    VerticalAlignment="Center"
                                    Text="Abstand Schmutzlöcher von Vorderkante Boden" />
                                <TextBlock
                                    Grid.Row="4"
                                    Grid.Column="0"
                                    VerticalAlignment="Center"
                                    Text="Anzahl Lochreihen der Schmutzlöcher" />
                                <TextBlock
                                    Grid.Row="5"
                                    Grid.Column="0"
                                    VerticalAlignment="Center"
                                    Text="Abstand Zueinander der Schmutzlöcher" />
                                <TextBlock
                                    Grid.Row="6"
                                    Grid.Column="0"
                                    VerticalAlignment="Center"
                                    Text="Abstand Schwellenbefestigungslöcher von VK-Boden" />
                                <TextBlock
                                    Grid.Row="7"
                                    Grid.Column="0"
                                    VerticalAlignment="Center"
                                    Text="Anzahl Lochreihen Schwellenbefestigung" />
                                <TextBlock
                                    Grid.Row="8"
