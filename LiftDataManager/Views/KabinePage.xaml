
<Page
    x:Class="LiftDataManager.Views.KabinePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:SpeziControls="using:LiftDataManager.Controls"
    xmlns:controls="using:CommunityToolkit.WinUI.UI.Controls"
    xmlns:converters="using:CommunityToolkit.WinUI.UI.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:LiftDataManager.Helpers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="using:CommunityToolkit.WinUI.UI"
    Style="{StaticResource PageStyle}"
    mc:Ignorable="d">

    <Page.Resources>
        <converters:BoolToObjectConverter
            x:Key="InvertBoolToObjectConverter"
            FalseValue="Visible"
            TrueValue="Collapsed" />
        <helpers:CustomInputStringConveter x:Key="CustomInputStringConveter" />
        <helpers:StringToBoolConverter x:Key="StringToBoolConverter" />
    </Page.Resources>
    <Grid
        x:Name="ContentArea"
        Margin="5"
        Background="{ThemeResource SystemControlPageBackgroundChromeLowBrush}"
        CornerRadius="10">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <!--  Parameter  -->
        <ScrollViewer
            Grid.Row="0"
            Margin="5,5,0,5"
            Padding="10,5,20,5">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <controls:Expander
                    x:Name="Fahrkorbgrunddaten"
                    Grid.Row="0"
                    Margin="0,0,0,10"
                    VerticalAlignment="Top"
                    HorizontalContentAlignment="Stretch"
                    ExpandDirection="Down"
                    Header="Fahrkorb Grunddaten"
                    IsExpanded="True">
                    <Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,*">
                        <Grid
                            Name="Kabinetyp"
                            Grid.Row="0"
                            ColumnDefinitions="*,2*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Fahrkorbtyp"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Fahrkorbtyp'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Fahrkorbtyp'].DropDownListValue, Mode=TwoWay}" />
                            <TextBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                Header="Kabinetyp Zusatzinformationen"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_Kabine_ZT'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Abmessungen"
                            Grid.Row="1"
                            ColumnDefinitions="*,*,*,*,*">
                            <TextBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Decimal"
                                Header="Kabinenbreite innen [mm]"
                                PlaceholderText="KB"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_KBI'].Value, Mode=TwoWay}">
                                <ToolTipService.ToolTip>
                                    <StackPanel>
                                        <TextBlock
                                            Margin="5,5,5,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Position Zugänge" />
                                        <Image
                                            Width="325"
                                            Height="350"
                                            Source="/Images/carAccesses.png" />
                                    </StackPanel>
                                </ToolTipService.ToolTip>
                            </TextBox>
                            <TextBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Decimal"
                                Header="Kabinentiefe innen [mm]"
                                PlaceholderText="KT"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_KTI'].Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                <ToolTipService.ToolTip>
                                    <StackPanel>
                                        <TextBlock
                                            Margin="5,5,5,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Position Zugänge" />
                                        <Image
                                            Width="325"
                                            Height="350"
                                            Source="/Images/carAccesses.png" />
                                    </StackPanel>
                                </ToolTipService.ToolTip>
                            </TextBox>
                            <TextBox
                                Grid.Column="2"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Decimal"
                                Header="Kabinenhöhe im Licht [mm]"
                                PlaceholderText="KH im Licht"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_KHLicht'].Value, Mode=TwoWay, UpdateSourceTrigger=LostFocus}">
                                <ToolTipService.ToolTip>
                                    <StackPanel>
                                        <TextBlock
                                            Margin="5,5,5,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Fahrkorb Abmessungen" />
                                        <Image
                                            Width="300"
                                            Height="300"
                                            Source="/Images/carDimensions.png" />
                                        <TextBlock
                                            Margin="5"
                                            FontWeight="Bold"
                                            Text="Kabinenhöhe innen bis UK abgehängte Decke"
                                            TextWrapping="Wrap" />
                                    </StackPanel>
                                </ToolTipService.ToolTip>
                            </TextBox>
                            <TextBox
                                Grid.Column="3"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Decimal"
                                Header="Deckenhöhe [mm]"
                                PlaceholderText="DH"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_KD'].Value, Mode=TwoWay, UpdateSourceTrigger=LostFocus}">
                                <ToolTipService.ToolTip>
                                    <StackPanel>
                                        <TextBlock
                                            Margin="5,5,5,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Fahrkorb Abmessungen" />
                                        <Image
                                            Width="300"
                                            Height="300"
                                            Source="/Images/carDimensions.png" />
                                        <TextBlock
                                            Margin="5"
                                            FontWeight="Bold"
                                            Text="Deckenhöhe gesamt (Deckenhöhe + abgehängte Decke) "
                                            TextWrapping="Wrap" />
                                    </StackPanel>
                                </ToolTipService.ToolTip>
                            </TextBox>
                            <TextBox
                                Grid.Column="4"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Decimal"
                                Header="Kabinenhöhe aussen [mm]"
                                PlaceholderText="KH aussen"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_KHA'].Value, Mode=TwoWay, UpdateSourceTrigger=LostFocus}">
                                <ToolTipService.ToolTip>
                                    <StackPanel>
                                        <TextBlock
                                            Margin="5,5,5,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Fahrkorb Abmessungen" />
                                        <Image
                                            Width="300"
                                            Height="300"
                                            Source="/Images/carDimensions.png" />
                                        <TextBlock
                                            Margin="5"
                                            FontWeight="Bold"
                                            Text="Gesamte Höhe der Kabine von Unterkante Boden bis Oberkante Halfensshiene auf dem Kabinendach"
                                            TextWrapping="Wrap" />
                                    </StackPanel>
                                </ToolTipService.ToolTip>
                            </TextBox>
                        </Grid>
                        <Grid
                            Name="HalsZugang"
                            Grid.Row="2"
                            ColumnDefinitions="*,*,*,*">
                            <TextBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Decimal"
                                Header="Hals links Breite L1 [mm]"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_L1'].Value, Mode=TwoWay}">
                                <ToolTipService.ToolTip>
                                    <StackPanel>
                                        <TextBlock
                                            Margin="5,5,5,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Position Zugänge" />
                                        <Image
                                            Width="325"
                                            Height="350"
                                            Source="/Images/carAccesses.png" />
                                    </StackPanel>
                                </ToolTipService.ToolTip>
                            </TextBox>
                            <TextBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Decimal"
                                Header="Hals links Breite R1 [mm]"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_R1'].Value, Mode=TwoWay}">
                                <ToolTipService.ToolTip>
                                    <StackPanel>
                                        <TextBlock
                                            Margin="5,5,5,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Position Zugänge" />
                                        <Image
                                            Width="325"
                                            Height="350"
                                            Source="/Images/carAccesses.png" />
                                    </StackPanel>
                                </ToolTipService.ToolTip>
                            </TextBox>
                            <TextBox
                                Grid.Column="2"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Decimal"
                                Header="Hals links Breite L2 [mm]"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_L2'].Value, Mode=TwoWay}">
                                <ToolTipService.ToolTip>
                                    <StackPanel>
                                        <TextBlock
                                            Margin="5,5,5,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Position Zugänge" />
                                        <Image
                                            Width="325"
                                            Height="350"
                                            Source="/Images/carAccesses.png" />
                                    </StackPanel>
                                </ToolTipService.ToolTip>
                            </TextBox>
                            <TextBox
                                Grid.Column="3"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Decimal"
                                Header="Hals links Breite R2 [mm]"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_R2'].Value, Mode=TwoWay}">
                                <ToolTipService.ToolTip>
                                    <StackPanel>
                                        <TextBlock
                                            Margin="5,5,5,10"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Bold"
                                            Text="Position Zugänge" />
                                        <Image
                                            Width="325"
                                            Height="350"
                                            Source="/Images/carAccesses.png" />
                                    </StackPanel>
                                </ToolTipService.ToolTip>
                            </TextBox>
                        </Grid>
                        <Grid
                            Name="Seitenwaende"
                            Grid.Row="3"
                            ColumnDefinitions="2*,Auto,*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Material Seitenwände"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Seitenwaende'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Seitenwaende'].DropDownListValue, Mode=TwoWay}" />
                            <TextBlock
                                Grid.Column="1"
                                Margin="5,0,5,5"
                                VerticalAlignment="Bottom"
                                FontSize="16"
                                Text="RAL:" />
                            <TextBox
                                Grid.Column="2"
                                Margin="5,0,5,0"
                                Header="RAL-Farbe Kabinenseitenwände"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_RAL_Seitenwand'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Rueckwand"
                            Grid.Row="4"
                            ColumnDefinitions="2*,Auto,*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Material Rückwand"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Rueckwand'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Rueckwand'].DropDownListValue, Mode=TwoWay}" />
                            <TextBlock
                                Grid.Column="1"
                                Margin="5,0,5,5"
                                VerticalAlignment="Bottom"
                                FontSize="16"
                                Text="RAL:" />
                            <TextBox
                                Grid.Column="2"
                                Margin="5,0,5,0"
                                Header="RAL-Farbe Rückwand"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_RAL_Rueckwand'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Eingangswand"
                            Grid.Row="5"
                            ColumnDefinitions="2*,Auto,*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Material Eingangswand"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Eingangswand'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Eingangswand'].DropDownListValue, Mode=TwoWay}" />
                            <TextBlock
                                Grid.Column="1"
                                Margin="5,0,5,5"
                                VerticalAlignment="Bottom"
                                FontSize="16"
                                Text="RAL:" />
                            <TextBox
                                Grid.Column="2"
                                Margin="5,0,5,0"
                                Header="RAL-Farbe Eingangswand"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_RAL_Eingangswand'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="MaterialStaerke"
                            Grid.Row="6"
                            ColumnDefinitions="*,2*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Materialstärke Schotten/Hals"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Materialstaerke'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Materialstaerke'].DropDownListValue, Mode=TwoWay}" />
                            <TextBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                Header="Schotten Zusatzinformationen"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_Schotten_ZT'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Antidroehn"
                            Grid.Row="7"
                            ColumnDefinitions="*,*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Antidröhn"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Antidroehn'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Antidroehn'].DropDownListValue, Mode=TwoWay}" />
                            <TextBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                Header="Antidröhn Zusatzinformationen"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_Antidroehn_ZT'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Decke"
                            Grid.Row="8"
                            ColumnDefinitions="2*,Auto,*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Material Kabinendach"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Decke'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Decke'].DropDownListValue, Mode=TwoWay}" />
                            <TextBlock
                                Grid.Column="1"
                                Margin="5,0,5,5"
                                VerticalAlignment="Bottom"
                                FontSize="16"
                                Text="RAL:" />
                            <TextBox
                                Grid.Column="2"
                                Margin="5,0,5,0"
                                Header="RAL-Farbe Kabinendecke"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_RAL_Decke'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="BelagKabinendach"
                            Grid.Row="9"
                            ColumnDefinitions="*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Belag auf dem Kabinendach"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_BelagAufDemKabinendach'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_BelagAufDemKabinendach'].DropDownListValue, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Bodentyp"
                            Grid.Row="10"
                            ColumnDefinitions="2*,*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Bodentyp"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Bodentyp'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Bodentyp'].DropDownListValue, Mode=TwoWay}" />
                            <TextBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                Header="Kabinenbodenhöhe"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_KU'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Bodenblech"
                            Grid.Row="11"
                            ColumnDefinitions="*,2*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Bodenblechstärke"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Bodenblech'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Bodenblech'].DropDownListValue, Mode=TwoWay}" />
                            <ComboBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Boden Sonderbleche"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Sonderbleche'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Sonderbleche'].DropDownListValue, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Bodenprofil"
                            Grid.Row="12"
                            ColumnDefinitions="*,2*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Bodenprofil verstärkt"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_BoPr'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_BoPr'].DropDownListValue, Mode=TwoWay}" />
                            <TextBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                Header="Bodenprofil Zusatzinformationen"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_Bodenprofil_ZT'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Bodenbelag"
                            Grid.Row="13"
                            ColumnDefinitions="3*,*,*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Bodenbelag"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Bodenbelag'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Bodenbelag'].DropDownListValue, Mode=TwoWay}" />
                            <TextBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                Header="Gewicht Bodenbelag"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_Bodenbelagsgewicht'].Value, Mode=TwoWay}" />
                            <TextBox
                                Grid.Column="2"
                                Margin="5,0,5,0"
                                Header="Stärke Bodenbelag"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_Bodenbelagsdicke'].Value, Mode=TwoWay}" />

                        </Grid>
                        <Grid
                            Name="BeschreibungBodenbelag"
                            Grid.Row="14"
                            ColumnDefinitions="*">
                            <TextBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                Header="Beschreibung Bodenbelag "
                                Text="{x:Bind ViewModel.ParamterDictionary['var_BodenbelagBeschreibung'].Value, Mode=TwoWay}" />
                        </Grid>
                    </Grid>
                </controls:Expander>
                <controls:Expander
                    x:Name="FahrkorbAusstattung"
                    Grid.Row="1"
                    Margin="0,0,0,10"
                    VerticalAlignment="Top"
                    HorizontalContentAlignment="Stretch"
                    ExpandDirection="Down"
                    Header="Fahrkorb Ausstattung"
                    IsExpanded="True">
                    <Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,Auto,*">
                        <Grid
                            Name="Beleuchtung"
                            Grid.Row="0"
                            ColumnDefinitions="2*,*">
                            <ComboBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Header="Beleuchtung"
                                ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Beleuchtung'].DropDownList, Mode=OneWay}"
                                SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Beleuchtung'].DropDownListValue, Mode=TwoWay}" />
                            <TextBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                ui:TextBoxExtensions.ValidationMode="Dynamic"
                                ui:TextBoxExtensions.ValidationType="Number"
                                Header="Anzahl Beleuchtungen"
                                Text="{x:Bind ViewModel.ParamterDictionary['var_AnzahlBeleuchtung'].Value, Mode=TwoWay}" />
                        </Grid>
                        <Grid
                            Name="Beleuchtung2"
                            Grid.Row="1"
                            ColumnDefinitions="*,*,*">
                            <CheckBox
                                Grid.Column="0"
                                Margin="5,0,5,0"
                                VerticalAlignment="Bottom"
                                Content="abgehängte Decke"
                                IsChecked="{x:Bind ViewModel.ParamterDictionary['var_abgDecke'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                            <CheckBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                VerticalAlignment="Bottom"
                                Content="Notlicht Kabine"
                                IsChecked="{x:Bind ViewModel.ParamterDictionary['var_NotlichtKab'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                            <Grid
                                Grid.Column="2"
                                Margin="5,10,5,0"
                                ColumnDefinitions="Auto,*">
                                <CheckBox
                                    Grid.Column="0"
                                    MinWidth="0"
                                    VerticalAlignment="Bottom"
                                    IsChecked="{x:Bind ViewModel.ParamterDictionary['var_BenDef_52'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                <TextBox
                                    Grid.Column="1"
                                    PlaceholderText="benutzerdefinierte Eingabe"
                                    Text="{x:Bind ViewModel.ParamterDictionary['var_BenDef_52'].Comment, Mode=TwoWay, Converter={StaticResource CustomInputStringConveter}}" />
                            </Grid>
                        </Grid>
                        <Grid
                            Name="Beleuchtung3"
                            Grid.Row="2"
                            ColumnDefinitions="*,*,*">
                            <CheckBox
                                Grid.Column="0"
                                Margin="5"
                                VerticalAlignment="Bottom"
                                Content="Dimmer"
                                IsChecked="{x:Bind ViewModel.ParamterDictionary['var_Dimmer'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                            <CheckBox
                                Grid.Column="1"
                                Margin="5,0,5,0"
                                VerticalAlignment="Bottom"
                                Content="Notlicht Tableau"
                                IsChecked="{x:Bind ViewModel.ParamterDictionary['var_NotlichtTab'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                            <Grid
                                Grid.Column="2"
                                Margin="5,10,5,0"
                                ColumnDefinitions="Auto,*">
                                <CheckBox
                                    Grid.Column="0"
                                    MinWidth="0"
                                    VerticalAlignment="Bottom"
                                    IsChecked="{x:Bind ViewModel.ParamterDictionary['var_BenDef_1'].Value, Mode=TwoWay, Converter={StaticResource StringToBoolConverter}}" />
                                <TextBox
