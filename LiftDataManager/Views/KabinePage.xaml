<Page
    x:Class="LiftDataManager.Views.KabinePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:SpeziControls="using:LiftDataManager.Controls"
    xmlns:converters="using:CommunityToolkit.WinUI.UI.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:LiftDataManager.Helpers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:media="using:CommunityToolkit.WinUI.UI.Media"
    xmlns:ui="using:CommunityToolkit.WinUI.UI"
    mc:Ignorable="d">

    <Page.Resources>
        <media:AttachedCardShadow
            x:Key="CommonShadow"
            CornerRadius="5"
            Offset="5" />
        <converters:BoolToObjectConverter
            x:Key="InvertBoolToObjectConverter"
            FalseValue="Visible"
            TrueValue="Collapsed" />
        <helpers:CustomInputStringConveter x:Key="CustomInputStringConveter" />
        <helpers:StringToBoolConverter x:Key="StringToBoolConverter" />
    </Page.Resources>
    <Grid x:Name="ContentArea">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <!--  Parameter  -->
        <ScrollViewer
            Grid.Row="0"
            Margin="0,0,0,0"
            Padding="0,0,15,0">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Expander
                    x:Name="Fahrkorbgrunddaten"
                    Grid.Row="0"
                    Margin="0,0,0,10"
                    Header="Fahrkorb Grunddaten"
                    IsExpanded="True"
                    Style="{StaticResource ExpanderContentStyle}">
                    <Grid RowDefinitions="Auto,Auto,Auto,Auto,*">
                        <Grid
                            x:Name="KabineGrunddaten"
                            Grid.Row="0"
                            Margin="0,5,0,5">
                            <Grid ColumnDefinitions="200,*,*" RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="8"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <Border
                                    Grid.RowSpan="8"
                                    Grid.Column="2"
                                    Margin="5,0,5,5"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.RowSpan="7"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Fahrkorb Grunddaten" />
                                <Grid
                                    x:Name="Kabine0"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*">
                                    <ComboBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        Header="Fahrkorbtyp"
                                        ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Fahrkorbtyp'].DropDownList, Mode=OneWay}"
                                        SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Fahrkorbtyp'].DropDownListValue, Mode=TwoWay}" />
                                </Grid>
                                <Grid
                                    x:Name="Kabine0_1"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*">
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        Header="Kabinetyp Zusatzinformationen"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_Kabine_ZT'].Value, Mode=TwoWay}" />
                                </Grid>
                                <Grid
                                    x:Name="Kabine1"
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <TextBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Kabinenbreite [mm]"
                                        PlaceholderText="KB"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_KBI'].Value, Mode=TwoWay}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Kabinentiefe [mm]"
                                        PlaceholderText="KT"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_KTI'].Value, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                </Grid>
                                <Grid
                                    x:Name="Kabine1_1"
                                    Grid.Row="3"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <TextBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Kabinenhöhe im Licht [mm]"
                                        PlaceholderText="KH im Licht"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_KHLicht'].Value, Mode=TwoWay, UpdateSourceTrigger=LostFocus}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Fahrkorb Abmessungen" />
                                                <Image
                                                    Width="300"
                                                    Height="300"
                                                    Source="/Images/carDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    FontWeight="Bold"
                                                    Text="Kabinenhöhe innen bis UK abgehängte Decke"
                                                    TextWrapping="Wrap" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Deckenhöhe [mm]"
                                        PlaceholderText="DH"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_KD'].Value, Mode=TwoWay, UpdateSourceTrigger=LostFocus}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Fahrkorb Abmessungen" />
                                                <Image
                                                    Width="300"
                                                    Height="300"
                                                    Source="/Images/carDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    FontWeight="Bold"
                                                    Text="Deckenhöhe gesamt (Deckenhöhe + abgehängte Decke) "
                                                    TextWrapping="Wrap" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                </Grid>
                                <Grid
                                    x:Name="Kabine1_2"
                                    Grid.Row="4"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <TextBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Bodenhöhe [mm]"
                                        IsEnabled="False"
                                        PlaceholderText="BH"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_KU'].Value, Mode=TwoWay, UpdateSourceTrigger=LostFocus}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Fahrkorb Abmessungen" />
                                                <Image
                                                    Width="300"
                                                    Height="300"
                                                    Source="/Images/carDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    FontWeight="Bold"
                                                    Text="Gesamte Höhe der Kabine von Unterkante Boden bis Oberkante Halfensshiene auf dem Kabinendach"
                                                    TextWrapping="Wrap" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Kabinenhöhe aussen [mm]"
                                        PlaceholderText="KH aussen"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_KHA'].Value, Mode=TwoWay, UpdateSourceTrigger=LostFocus}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Fahrkorb Abmessungen" />
                                                <Image
                                                    Width="300"
                                                    Height="300"
                                                    Source="/Images/carDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    FontWeight="Bold"
                                                    Text="Gesamte Höhe der Kabine von Unterkante Boden bis Oberkante Halfensshiene auf dem Kabinendach"
                                                    TextWrapping="Wrap" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                </Grid>
                                <Grid
                                    x:Name="Kabine2"
                                    Grid.Row="5"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*,*,*">
                                    <TextBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Hals A (L1)"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_L1'].Value, Mode=TwoWay}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Hals A (R1)"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_R1'].Value, Mode=TwoWay}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                    <TextBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Hals C (L2)"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_L2'].Value, Mode=TwoWay}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                    <TextBox
                                        Grid.Column="3"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Hals C (R2)"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_R2'].Value, Mode=TwoWay}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                </Grid>
                                <Grid
                                    x:Name="Kabine2_1"
                                    Grid.Row="6"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*,*,*">
                                    <TextBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Hals B (L3)"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_L1'].Value, Mode=TwoWay}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Hals B (R3)"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_R1'].Value, Mode=TwoWay}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                    <TextBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Hals D (L4)"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_L2'].Value, Mode=TwoWay}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                    <TextBox
                                        Grid.Column="3"
                                        Margin="5,0,5,0"
                                        VerticalAlignment="Bottom"
                                        ui:TextBoxExtensions.ValidationMode="Dynamic"
                                        ui:TextBoxExtensions.ValidationType="Decimal"
                                        Header="Hals D (R4)"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_R2'].Value, Mode=TwoWay}">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </TextBox>
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Seitenwaende"
                            Grid.Row="1"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,Auto,Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="6"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.RowSpan="5"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Fahrkorb Seitenwände" />
                                <Grid
                                    x:Name="Kabine3"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,Auto,*">
                                    <ComboBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        Header="Material Seitenwände"
                                        ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Seitenwaende'].DropDownList, Mode=OneWay}"
                                        SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Seitenwaende'].DropDownListValue, Mode=TwoWay}" />
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="5,0,5,5"
                                        VerticalAlignment="Bottom"
                                        FontSize="16"
                                        Text="RAL:" />
                                    <TextBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        Header="RAL-Farbe Kabinenseitenwände"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_RAL_Seitenwand'].Value, Mode=TwoWay}" />
                                </Grid>
                                <Grid
                                    x:Name="Kabine4"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,Auto,*">
                                    <ComboBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        Header="Material Rückwand"
                                        ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Rueckwand'].DropDownList, Mode=OneWay}"
                                        SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Rueckwand'].DropDownListValue, Mode=TwoWay}" />
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="5,0,5,5"
                                        VerticalAlignment="Bottom"
                                        FontSize="16"
                                        Text="RAL:" />
                                    <TextBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        Header="RAL-Farbe Rückwand"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_RAL_Rueckwand'].Value, Mode=TwoWay}" />
                                </Grid>
                                <Grid
                                    x:Name="Kabine5"
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,Auto,*">
                                    <ComboBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        Header="Material Eingangswand"
                                        ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Eingangswand'].DropDownList, Mode=OneWay}"
                                        SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Eingangswand'].DropDownListValue, Mode=TwoWay}" />
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="5,0,5,5"
                                        VerticalAlignment="Bottom"
                                        FontSize="16"
                                        Text="RAL:" />
                                    <TextBox
                                        Grid.Column="2"
                                        Margin="5,0,5,0"
                                        Header="RAL-Farbe Eingangswand"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_RAL_Eingangswand'].Value, Mode=TwoWay}" />
                                </Grid>
                                <Grid
                                    x:Name="Kabine6"
                                    Grid.Row="3"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,2*">
                                    <ComboBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        Header="Materialstärke Schotten/Hals"
                                        ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Materialstaerke'].DropDownList, Mode=OneWay}"
                                        SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Materialstaerke'].DropDownListValue, Mode=TwoWay}" />
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        Header="Schotten Zusatzinformationen"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_Schotten_ZT'].Value, Mode=TwoWay}" />
                                </Grid>
                                <Grid
                                    x:Name="Kabine7"
                                    Grid.Row="4"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <ComboBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        Header="Antidröhn"
                                        ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Antidroehn'].DropDownList, Mode=OneWay}"
                                        SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Antidroehn'].DropDownListValue, Mode=TwoWay}" />
                                    <TextBox
                                        Grid.Column="1"
                                        Margin="5,0,5,0"
                                        Header="Antidröhn Zusatzinformationen"
                                        Text="{x:Bind ViewModel.ParamterDictionary['var_Antidroehn_ZT'].Value, Mode=TwoWay}" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Decke"
                            Grid.Row="2"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="3"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.RowSpan="2"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Fahrkorb Decke" />
                                <Grid
                                    x:Name="Kabine8"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,Auto,*">
                                    <ComboBox
                                        Grid.Column="0"
                                        Margin="5,0,5,0"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Center"
                                        Header="Material Kabinendach"
                                        ItemsSource="{x:Bind ViewModel.ParamterDictionary['var_Decke'].DropDownList, Mode=OneWay}"
                                        SelectedValue="{x:Bind ViewModel.ParamterDictionary['var_Decke'].DropDownListValue, Mode=TwoWay}" />
                                    <TextBlock
                                        Grid.Column="1"
                                        Margin="5,0,5,5"
                                        VerticalAlignment="Bottom"
                                        FontSize="16"
