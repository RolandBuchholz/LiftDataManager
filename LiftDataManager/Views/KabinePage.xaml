                                        </SpeziControls:ParameterCheckBox>
                                        <SpeziControls:ParameterCheckBox
                                            Grid.Column="2"
                                            CheckBoxContent="Seite C"
                                            IsControlActive="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_C'].Value, Mode=OneWay, Converter={StaticResource StringToBoolInvertConverter}}"
                                            LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_PaneelPosC'], Mode=TwoWay}"
                                            ShowDefaultCheckBoxContent="False">
                                            <ToolTipService.ToolTip>
                                                <StackPanel>
                                                    <TextBlock
                                                        Margin="5,5,5,10"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontSize="16"
                                                        FontWeight="Bold"
                                                        Text="Position Zugänge" />
                                                    <Image
                                                        Width="325"
                                                        Height="350"
                                                        Source="/Images/carAccesses.png" />
                                                </StackPanel>
                                            </ToolTipService.ToolTip>
                                        </SpeziControls:ParameterCheckBox>
                                        <SpeziControls:ParameterCheckBox
                                            Grid.Column="3"
                                            CheckBoxContent="Seite D"
                                            IsControlActive="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_D'].Value, Mode=OneWay, Converter={StaticResource StringToBoolInvertConverter}}"
                                            LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_PaneelPosD'], Mode=TwoWay}"
                                            ShowDefaultCheckBoxContent="False">
                                            <ToolTipService.ToolTip>
                                                <StackPanel>
                                                    <TextBlock
                                                        Margin="5,5,5,10"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center"
                                                        FontSize="16"
                                                        FontWeight="Bold"
                                                        Text="Position Zugänge" />
                                                    <Image
                                                        Width="325"
                                                        Height="350"
                                                        Source="/Images/carAccesses.png" />
                                                </StackPanel>
                                            </ToolTipService.ToolTip>
                                        </SpeziControls:ParameterCheckBox>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Ventilator"
                            Grid.Row="3"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Ventilator" />
                                <Grid
                                    x:Name="Ausstattung11"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="auto,auto,*">
                                    <SpeziControls:ParameterCheckBox
                                        Grid.Column="0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_VentilatorLuftmenge'], Mode=TwoWay}"
                                        ShowDefaultCheckBoxContent="True" />
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="1"
                                        Margin="0,5,0,0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_VentilatorAnzahl'], Mode=TwoWay}"
                                        ShowDefaultHeader="False" />
                                    <TextBlock
                                        Grid.Column="2"
                                        Margin="15,5,0,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        FontSize="14"
                                        Text="Anzahl Ventilatoren" />
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="sonstigesFahrkorb"
                            Grid.Row="4"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,10">
                                <Border
                                    Grid.RowSpan="2"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="sonstiges Fahrkorb" />
                                <Grid
                                    x:Name="Ausstattung12"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*">
                                    <TextBox
                                        Grid.Column="0"
                                        MinHeight="120"
                                        Margin="5,5,5,0"
                                        AcceptsReturn="true"
                                        Text="{x:Bind ViewModel.ParameterDictionary['var_SonstigesFahrkorb'].Value, Converter={StaticResource NewLineConveter}, Mode=TwoWay}"
                                        TextWrapping="Wrap" />
                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>
                </Expander>
            </Grid>
        </ScrollViewer>
        <!--  InfoSidebarPanel  -->
        <SpeziControls:SidebarPanel
            Grid.Row="0"
            Grid.Column="1"
            Margin="5,0,0,0"
            InfoText="{x:Bind ViewModel.InfoSidebarPanelText, Mode=TwoWay}"
            ShowQuickLinks="{x:Bind ViewModel.AuftragsbezogeneXml, Mode=OneWay}" />
        <!--  Footer  -->
        <SpeziControls:FooterControl
            Grid.Row="1"
            Grid.ColumnSpan="2"
            Margin="0"
            CheckOut="{x:Bind ViewModel.CheckOut, Mode=OneWay}"
            ErrorsDictionary="{x:Bind ViewModel.ParameterErrorDictionary, Mode=TwoWay}"
            HasErrors="{x:Bind ViewModel.HasErrors, Mode=OneWay}"
            HideInfoErrors="{x:Bind ViewModel.HideInfoErrors, Mode=TwoWay}"
            ParameterFound="{x:Bind ViewModel.ParameterDictionary.Count, Mode=OneWay}"
            XmlPath="{x:Bind ViewModel.FullPathXml, Mode=OneWay}" />
    </Grid>
</Page>
