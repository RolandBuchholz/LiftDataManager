<Page
    x:Class="LiftDataManager.Views.KabinePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:SpeziControls="using:LiftDataManager.Controls"
    xmlns:controls="using:CommunityToolkit.WinUI.Controls"
    xmlns:converters="using:CommunityToolkit.WinUI.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:LiftDataManager.Helpers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:media="using:CommunityToolkit.WinUI.Media"
    xmlns:sys="using:System"
    xmlns:ui="using:CommunityToolkit.WinUI"
    mc:Ignorable="d">

    <Page.Resources>
        <media:AttachedCardShadow
            x:Key="CommonShadow"
            CornerRadius="5"
            Offset="5" />
        <converters:BoolToObjectConverter
            x:Key="InvertBoolToObjectConverter"
            FalseValue="Visible"
            TrueValue="Collapsed" />
        <helpers:NewLineConveter x:Key="NewLineConveter" />
        <helpers:CustomInputStringConveter x:Key="CustomInputStringConveter" />
        <helpers:StringToBoolConverter x:Key="StringToBoolConverter" />
        <helpers:StringToBoolInvertConverter x:Key="StringToBoolInvertConverter" />
    </Page.Resources>
    <Grid x:Name="ContentArea">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>
        <!--  Parameter  -->
        <ScrollViewer
            Grid.Row="0"
            Margin="0,0,0,0"
            Padding="0,0,15,0">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Expander
                    x:Name="Fahrkorbgrunddaten"
                    Grid.Row="0"
                    Margin="0,0,0,10"
                    Header="Fahrkorb Grunddaten"
                    IsExpanded="True"
                    Style="{StaticResource ExpanderContentStyle}">
                    <Grid RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto,*">
                        <Grid
                            x:Name="KabineGrunddaten"
                            Grid.Row="0"
                            Margin="0,5,0,5">
                            <Grid ColumnDefinitions="200,*,*" RowDefinitions="Auto,Auto,Auto,Auto,Auto,Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="8"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <Border
                                    Grid.RowSpan="8"
                                    Grid.Column="2"
                                    Margin="5,0,5,5"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.RowSpan="7"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Fahkorb Abmessungen" />
                                <SpeziControls:ParameterComboBox
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Fahrkorbtyp'], Mode=TwoWay}"
                                    ShowDefaultHeader="True" />
                                <SpeziControls:ParameterTextBox
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Kabine_ZT'], Mode=TwoWay}"
                                    ShowDefaultHeader="True" />
                                <Grid
                                    x:Name="Kabine1"
                                    Grid.Row="2"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KBI'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        PlaceholderText="KB"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KTI'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        PlaceholderText="KT"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                </Grid>
                                <Grid
                                    x:Name="Kabine1_1"
                                    Grid.Row="3"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KHLicht'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        PlaceholderText="KH im Licht"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Fahrkorb Abmessungen" />
                                                <Image
                                                    Width="300"
                                                    Height="300"
                                                    Source="/Images/carDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    FontWeight="Bold"
                                                    Text="Kabinenhöhe innen bis UK abgehängte Decke"
                                                    TextWrapping="Wrap" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KD'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        PlaceholderText="DH"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Fahrkorb Abmessungen" />
                                                <Image
                                                    Width="300"
                                                    Height="300"
                                                    Source="/Images/carDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    FontWeight="Bold"
                                                    Text="Deckenhöhe gesamt (Deckenhöhe + abgehängte Decke) "
                                                    TextWrapping="Wrap" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                </Grid>
                                <Grid
                                    x:Name="Kabine1_2"
                                    Grid.Row="4"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*">
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KU'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        PlaceholderText="BH"
                                        ReadOnly="True"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="12"
                                                    FontWeight="Bold">
                                                    Bodenhöhe automatisch berechnet
                                                </TextBlock>
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="1"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_KHA'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        PlaceholderText="KH aussen"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Fahrkorb Abmessungen" />
                                                <Image
                                                    Width="300"
                                                    Height="300"
                                                    Source="/Images/carDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    FontWeight="Bold"
                                                    Text="Gesamte Höhe der Kabine von Unterkante Boden bis Oberkante Halfensshiene auf dem Kabinendach"
                                                    TextWrapping="Wrap" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                </Grid>
                                <Grid
                                    x:Name="Kabine2"
                                    Grid.Row="5"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*,*,*">
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="0"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_A'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_L1'], Mode=TwoWay}"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="1"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_A'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_R1'], Mode=TwoWay}"
                                        ReadOnly="True"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="12"
                                                    FontWeight="Bold">
                                                    Hals R1 wird automatisch berechnet<LineBreak />
                                                    (Kabinenbreite - Türbreite - Hals L1)</TextBlock>
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="2"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_C'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_L2'], Mode=TwoWay}"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="3"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_C'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_R2'], Mode=TwoWay}"
                                        ReadOnly="True"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="12"
                                                    FontWeight="Bold">
                                                    Hals R2 wird automatisch berechnet<LineBreak />
                                                    (Kabinenbreite - Türbreite - Hals L2)</TextBlock>
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                </Grid>
                                <Grid
                                    x:Name="Kabine2_1"
                                    Grid.Row="6"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*,*,*">
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="0"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_B'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_L3'], Mode=TwoWay}"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="1"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_B'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_R3'], Mode=TwoWay}"
                                        ReadOnly="True"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="12"
                                                    FontWeight="Bold">
                                                    Hals R3 wird automatisch berechnet<LineBreak />
                                                    (Kabinentiefe - Türbreite - Hals L3)</TextBlock>
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="2"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_D'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_L4'], Mode=TwoWay}"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Position Zugänge" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/carAccesses.png" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="3"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_D'].Value, Converter={StaticResource StringToBoolConverter}, Mode=TwoWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_R4'], Mode=TwoWay}"
                                        ReadOnly="True"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="12"
                                                    FontWeight="Bold">
                                                    Hals R4 wird automatisch berechnet<LineBreak />
                                                    (Kabinentiefe - Türbreite - Hals L4)</TextBlock>
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                </Grid>
                                <!--  carControl  -->
                                <SpeziControls:CarDesignControl
                                    Grid.Row="0"
                                    Grid.RowSpan="7"
                                    Grid.Column="2"
                                    Margin="10,5,10,0"
                                    ItemSource="{x:Bind ViewModel.ParameterDictionary, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                    ShowEntranceButtons="True" />
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="KabineTuerpaket"
                            Grid.Row="1"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="3"
                                    Grid.Column="0"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <TextBlock
                                    Grid.RowSpan="7"
                                    Margin="10,0,0,0"
                                    VerticalAlignment="Center"
                                    FontSize="16"
                                    Text="Kabinentüranbau" />
                                <Grid
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="*,*,*,*">
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="0"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_A'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TuerEinbau'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Türpacket" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/CarDetails/carDoorinstallationDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="1) Schwellen Vorderkante" />
                                                <TextBlock
                                                    Margin="5"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="2) Kabinen Innenkante" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="1"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_B'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TuerEinbauB'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Türpacket" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/CarDetails/carDoorinstallationDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="1) Schwellen Vorderkante" />
                                                <TextBlock
                                                    Margin="5"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="2) Kabinen Innenkante" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="2"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_C'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TuerEinbauC'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Türpacket" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/CarDetails/carDoorinstallationDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="1) Schwellen Vorderkante" />
                                                <TextBlock
                                                    Margin="5"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="2) Kabinen Innenkante" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                    <SpeziControls:ParameterNumberTextBox
                                        Grid.Column="3"
                                        IsEnabled="{x:Bind ViewModel.ParameterDictionary['var_ZUGANSSTELLEN_D'].Value, Converter={StaticResource StringToBoolConverter}, Mode=OneWay}"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_TuerEinbauD'], Mode=TwoWay, UpdateSourceTrigger=LostFocus}"
                                        ShowDefaultHeader="True"
                                        Unit="mm">
                                        <ToolTipService.ToolTip>
                                            <StackPanel>
                                                <TextBlock
                                                    Margin="5,5,5,10"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="Türpacket" />
                                                <Image
                                                    Width="325"
                                                    Height="350"
                                                    Source="/Images/CarDetails/carDoorinstallationDimensions.png" />
                                                <TextBlock
                                                    Margin="5"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="1) Schwellen Vorderkante" />
                                                <TextBlock
                                                    Margin="5"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    FontSize="16"
                                                    FontWeight="Bold"
                                                    Text="2) Kabinen Innenkante" />
                                            </StackPanel>
                                        </ToolTipService.ToolTip>
                                    </SpeziControls:ParameterNumberTextBox>
                                </Grid>
                            </Grid>
                        </Grid>
                        <Grid
                            x:Name="Seitenwaende"
                            Grid.Row="2"
                            Margin="0,0,0,5">
                            <Grid ColumnDefinitions="200,*" RowDefinitions="Auto,Auto,Auto,Auto,Auto,10">
                                <Border
                                    Grid.RowSpan="6"
                                    Grid.ColumnSpan="2"
                                    ui:Effects.Shadow="{StaticResource CommonShadow}"
                                    Style="{StaticResource ResourceKey=SpeziRowBorderStyle}" />
                                <StackPanel
                                    Grid.RowSpan="5"
                                    Margin="10,20,0,0"
                                    VerticalAlignment="Center">
                                    <TextBlock FontSize="16" Text="Fahrkorb Seitenwände" />
                                    <HyperlinkButton NavigateUri="https://www.berchtenbreiter-gmbh.de/de/oberflaechen.html">
                                        <Image
                                            Width="30"
                                            Height="30"
                                            Margin="5"
                                            HorizontalAlignment="Left"
                                            Source="/Images/Homepage.ico" />
                                        <ToolTipService.ToolTip>
                                            <ToolTip>
                                                https://www.berchtenbreiter-gmbh.de/de/oberflaechen.html
                                            </ToolTip>
                                        </ToolTipService.ToolTip>
                                    </HyperlinkButton>
                                </StackPanel>
                                <Grid
                                    x:Name="Kabine3"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Margin="0,0,5,0"
                                    ColumnDefinitions="2*,Auto,*">
                                    <SpeziControls:ParameterComboBox
                                        Grid.Column="0"
                                        LiftParameter="{x:Bind ViewModel.ParameterDictionary['var_Seitenwaende'], Mode=TwoWay}"
                                        ShowDefaultHeader="True" />
                                    <TextBlock
