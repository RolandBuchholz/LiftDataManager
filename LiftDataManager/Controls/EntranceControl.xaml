<UserControl
    x:Class="LiftDataManager.Controls.EntranceControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="using:CommunityToolkit.WinUI.UI.Controls"
    xmlns:converters="using:CommunityToolkit.WinUI.UI.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:LiftDataManager.Helpers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:media="using:CommunityToolkit.WinUI.UI.Media"
    xmlns:ui="using:CommunityToolkit.WinUI.UI"
    mc:Ignorable="d">

    <controls:Expander
        x:Name="Stockwerksbezeichnung"
        VerticalAlignment="Top"
        HorizontalContentAlignment="Stretch"
        ExpandDirection="Down"
        IsExpanded="True">
        <controls:Expander.Resources>
            <converters:BoolToObjectConverter
                x:Key="InvertBoolToObjectConverter"
                FalseValue="Visible"
                TrueValue="Collapsed" />
            <media:AttachedCardShadow
                x:Key="CommonShadow"
                CornerRadius="5"
                Offset="5" />
            <helpers:StringToBoolConverter x:Key="StringToBoolConverter" />
        </controls:Expander.Resources>
        <controls:Expander.Header>
            <StackPanel Orientation="Horizontal">
                <TextBlock VerticalAlignment="Center" Text="Stockwerksbezeichnung / Etagenabstände" />
                <TextBlock
                    Margin="20,0,0,0"
                    VerticalAlignment="Center"
                    Text="Hauptzugang:">
                    <ToolTipService.ToolTip>
                        <TextBlock Text="Hauptzugang mit rechter Maustaste wählen" />
                    </ToolTipService.ToolTip>
                </TextBlock>
                <TextBlock
                    x:Name="DisplayName"
                    Margin="10,0,0,0"
                    VerticalAlignment="Center"
                    Text="{x:Bind DisplayNameHauptzugang, Mode=OneWay}">
                    <ToolTipService.ToolTip>
                        <TextBlock Text="Hauptzugang mit rechter Maustaste wählen" />
                    </ToolTipService.ToolTip>
                </TextBlock>
                <AppBarButton
                    x:Name="btn_Delete_Hauptzugang"
                    Height="45"
                    Margin="10,-2.5,0,0"
                    VerticalAlignment="Top"
                    Command="{x:Bind ResetHauptzugangCommand}"
                    Icon="Delete"
                    IsEnabled="True">
                    <ToolTipService.ToolTip>
                        <TextBlock Text="Reset Hauptzugang" />
                    </ToolTipService.ToolTip>
                </AppBarButton>
            </StackPanel>
        </controls:Expander.Header>
