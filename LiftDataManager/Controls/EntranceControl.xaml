<UserControl
    x:Class="LiftDataManager.Controls.EntranceControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:CommunityToolkit.WinUI.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:LiftDataManager.Helpers"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:media="using:CommunityToolkit.WinUI.Media"
    xmlns:ui="using:CommunityToolkit.WinUI"
    mc:Ignorable="d">

    <UserControl.Resources>
        <helpers:BoolToHighlightBrushConverter x:Key="BoolToHighlightBrushConverter" />
    </UserControl.Resources>
    <Expander
        x:Name="Stockwerksbezeichnung"
        IsExpanded="True"
        Style="{StaticResource ExpanderContentStyle}">
        <Expander.Resources>
            <converters:BoolToObjectConverter
                x:Key="InvertBoolToObjectConverter"
                FalseValue="Visible"
                TrueValue="Collapsed" />
            <media:AttachedCardShadow
                x:Key="CommonShadow"
                CornerRadius="5"
                Offset="5" />
            <helpers:StringToBoolConverter x:Key="StringToBoolConverter" />
            <TextCommandBarFlyout
                x:Name="EntranceControlFlyout"
                Opening="TextCommandBarFlyout_Opening"
                Placement="Bottom" />
        </Expander.Resources>
        <Expander.Header>
            <StackPanel Orientation="Horizontal">
                <TextBlock VerticalAlignment="Center" Text="Stockwerksbezeichnung / Etagenabstände" />
                <TextBlock
                    Margin="20,0,0,0"
                    VerticalAlignment="Center"
                    Text="Haupthaltestelle:">
                    <ToolTipService.ToolTip>
                        <TextBlock Text="Haupthaltestelle mit rechter Maustaste wählen" />
                    </ToolTipService.ToolTip>
                </TextBlock>
                <TextBlock
                    x:Name="DisplayName"
                    Margin="10,0,0,0"
                    VerticalAlignment="Center"
                    Text="{x:Bind DisplayNameHauptzugang, Mode=OneWay}">
                    <ToolTipService.ToolTip>
                        <TextBlock Text="Haupthaltestelle mit rechter Maustaste wählen" />
                    </ToolTipService.ToolTip>
                </TextBlock>
                <AppBarButton
                    x:Name="btn_Delete_Hauptzugang"
                    Height="45"
                    Margin="10,-2.5,0,0"
                    VerticalAlignment="Top"
                    Click="Delete_Hauptzugang_Click"
                    Icon="Delete"
                    IsEnabled="True">
                    <ToolTipService.ToolTip>
                        <TextBlock Text="Reset Haupthaltestelle" />
                    </ToolTipService.ToolTip>
                </AppBarButton>
            </StackPanel>
        </Expander.Header>
