                Grid.RowSpan="7"
                Grid.Column="0"
                Grid.ColumnSpan="12"
                ui:Effects.Shadow="{StaticResource CommonShadow}"
                Style="{StaticResource SpeziRowBorderStyle}" />
            <Border
                Grid.Row="0"
                Grid.Column="0"
                Grid.ColumnSpan="12"
                Height="30"
                Margin="0,0,5,5"
